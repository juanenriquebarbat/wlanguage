	"repository": {
		"numbers":{
			"patterns": [
				{
					"match": "(?<=[<>=,()\\[\\] ])([-+]?[0-9]*[\\.]?[0-9]+)",
					"name": "invalid.wlang"
				}
			]
		},
		"comments": {
			"patterns": [
				{
					"begin": "/\\*",
					"end": "\\*/",
					"captures": {
						"0": {
							"name": "punctuation.definition.comment.wlang"
						}
					},
					"name": "comment.block.wlang"
				},
				{
					"begin": "//",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.comment.wlang"
						}
					},
					"end": "$",
					"name": "comment.line.double-slash.wlang"
				}
			]
		},
		"keywords": {
			"patterns": [
				{
					"name": "keyword.control.en.wlang",
					"match": "(?i)\\b(FUNCTION|GLOBAL|INTERN|LOCAL|PROCEDURE)\\b"
				},
				{
					"name": "keyword.control.en.wlang",
					"match": "(?i)\\b(A|\\_A\\_|THEN|OTHER|CASE|EACH|IN|\\_IN\\_|AND|\\_AND\\_|END|WHERE|\\_WHERE\\_|NOT|FOR|WHEN|NEXT|IF|ELSE|ON|WHILE|ALL|ALL)\\b"
				},
				{
					"name": "keyword.control.en.wlang",
					"match": "(?i)\\b(LOOP|CONTINUE|EXCEPTION|DO|RESULT|RETURN|SEPARATE|EXIT)\\b"
				},
				{
					"match": "(?i)\\b(of|from|is|by|are|a|a|without)\\b",
					"name": "support.function.en.wlang"
				},
				{
					"match": "(?i)\\b(allocate|inherits|release|modulo|is)\\b",
					"name": "support.function.en.wlang"
				}
			]
		},
		"strings": {
			"patterns": [
				{
					"include": "#string-double-quoted"
				},
				{
					"include": "#string-single-quoted"
				}
			]
		},
		"string-double-quoted": {
			"name": "string.quoted.double.wlang",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.wlang",
					"match": "\\."
				}
			]
		},
		"string-single-quoted": {
			"name": "string.quoted.single.wlang",
			"begin": "\\'",
			"end": "\\'",
			"patterns": [
				{
					"name": "constant.character.escape.wlang",
					"match": "\\."
				}
			]
		},
